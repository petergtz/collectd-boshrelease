LoadPlugin df
LoadPlugin disk
LoadPlugin cpu
LoadPlugin load
LoadPlugin memory
LoadPlugin write_metric_mtlumberjack
<Plugin write_metric_mtlumberjack>
   <Logstash>
       Host "metrics.opvis.bluemix.net"
       Port "9095"
       GraphitePrefix "<%= p("collectd.space_id") %>.<%= spec.dns_domain_name %>.<%= spec.deployment %>.<%= name %>.<%= index %>."
       SpaceId "<%= p("collectd.space_id") %>"
       LoggingToken "<%= p("collectd.logging_token") %>"
   </Logstash>
</Plugin>
LoadPlugin statsd
<Plugin statsd>
  Host "127.0.0.1"
  DeleteCounters true
  DeleteTimers true
  DeleteGauges true
  DeleteSets true
  TimerPercentile 99.0
  TimerPercentile 95.0
  TimerPercentile 90.0
  TimerCount true
</Plugin>
